  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "mqtt": "^4.3.7",
    "rollup": "^2.70.2",
    "vite-plugin-dts": "^1.1.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "ts-morph": "^14.0.0",
